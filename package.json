{
  "name": "@vanioinformatika/jwt-wrapper",
  "version": "2.0.4",
  "description": "A wrapper around the jsonwebtoken npm module that handles key ids.",
  "author": "Gyula Szalai <gyszalai@gmail.com>",
  "license": "MIT",
  "contributors": [
    {
      "name": "Gyula Szalai",
      "email": "gyszalai@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vanioinformatika/node-jwt-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/vanioinformatika/node-jwt-wrapper/issues"
  },
  "keywords": [
    "node",
    "jwt",
    "jsonwebtoken"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">6.9"
  },
  "scripts": {
    "compile": "tsc",
    "coverage": "nyc npm run test",
    "coverage:ci": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "test": "mocha --recursive --compilers ts:ts-node/register test/**/*.spec.ts",
    "tslint": "tslint --fix src/**/*.ts test/**/*.ts",
    "preversion": "git push && npm test",
    "version": "npm run compile && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "base64url": "^1.0.6",
    "debug": "^3.1.0",
    "jsonwebtoken": "^8.1.0",
    "util.promisify": "^1.0.0"
  },
  "devDependencies": {
    "@types/base64url": "^2.0.3",
    "@types/chai": "^4.0.5",
    "@types/chai-as-promised": "^7.1.0",
    "@types/debug": "^0.0.30",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mocha": "^2.2.44",
    "@types/node": "^6.0.92",
    "@types/util.promisify": "^1.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "dirty-chai": "^2.0.1",
    "mocha": "^4.0.1",
    "mocha-junit-reporter": "^1.15.0",
    "nyc": "^11.3.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
